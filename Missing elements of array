
#include<stdio.h>
int main(){
    int n;
    printf("Enter Elements: ");
    scanf("%d",&n);
    int arr[n]; 
    printf("Enter elements: ");
    for(int i = 0 ; i < n ; i++){
        scanf("%d",&arr[i]);
    }
    int temp[100] = {0};
    
    for(int i = 0 ; i < n - 1; i++){
        for(int j = 0 ; j < n - i - 1; j++){
            if(arr[j]>arr[j+1]){
                int temp1 = arr[j];
                arr[j] = arr[j+1];
                arr[j+1] = temp1;
            }
        }
    }
    
    for(int i = 0 ; i < n ; i++){
    printf("%d ",arr[i]);
    }
    printf("\n");
    
   for(int i = 0 ;i<n;i++){
       int index = arr[i];
       temp[index] = 1;
   }
     for(int i = 0; i <= arr[n-1]; i++){
        if(temp[i] == 0){
            printf("%d ", i);
        }
    }
    
    
  
    
    
}
